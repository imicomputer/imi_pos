                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     faktur_detail.no_faktur, faktur.tgl_faktur, faktur.atas_nama, faktur.dgn_telp, faktur_detail.id_produk, produk.nama, faktur_detail.harga, 
